{
	"name": "@gov.au/pancake",
	"version": "0.0.9",
	"description": "Pancake is an utility to make working with npm modules for the frontend sweet and seamlessly.",
	"keywords": [
		"uikit",
		"scss",
		"css",
		"gov.au",
		"Commonwealth of Australia",
		"design guide",
		"design system",
		"npm",
		"peerdependencies",
		"cli"
	],
	"engines": {
		"npm": ">=3.0.0"
	},
	"main": "./bin/pancake.js",
	"bin": {
		"pancake": "./bin/pancake.js"
	},
	"scripts": {
		"prepublish": "npm run build",

		"test": "node tests/tester.js",

		"build": "npm run build:js",
		"build:js": "babel src --out-dir bin --presets=es2015 --plugins=transform-runtime",
		"watch": "npm run build && onchange 'src/*.js' -- npm run build"
	},
	"files": [
		"bin/*",
		"assets/*.mp3",
		"settings.json"
	],
	"dependencies": {
		"autoprefixer": "6.7.0",
		"babel-runtime": "6.22.0",
		"cfonts": "1.1.1",
		"chalk": "1.1.3",
		"commander": "2.9.0",
		"inquirer": "3.0.1",
		"node-sass": "4.3.0",
		"play-sound": "1.1.1",
		"postcss": "5.2.11",
		"semver": "5.3.0",
		"strip-ansi": "3.0.1",
		"uglify-js": "2.7.5",
		"window-size": "0.3.0"
	},
	"devDependencies": {
		"babel-cli": "^6.22.2",
		"babel-plugin-transform-runtime": "^6.22.0",
		"babel-preset-es2015": "^6.22.0",
		"del": "^2.2.2",
		"dirsum": "^0.1.1",
		"onchange": "^3.2.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/govau/pancake.git"
	},
	"homepage": "https://github.com/govau/pancake",
	"bugs": {
		"url": "https://github.com/govau/pancake/issues",
		"email": "guides@digital.gov.au"
	},
	"author": {
		"name": "Commonwealth of Australia, DTA",
		"email": "guides@digital.gov.au",
		"url": "https://www.dta.gov.au/"
	},
	"contributors": [
		{
			"name": "Dominik Wilkowski",
			"email": "hi@dominik-wilkowski.com",
			"url": "https://dominik-wilkowski.com/"
		}
	],
	"license": "MIT"
}
